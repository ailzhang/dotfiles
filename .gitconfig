[core]
  preloadindex = true
[user]
  name = Ailing Zhang
  email = ailzhang@users.noreply.github.com
[alias]
	co = checkout
	br = branch
	ci = commit
	st = status
[push]
	default = simple
[credential]
	helper = osxkeychain

[alias]
	graph = log --graph --abbrev-commit --decorate --date=relative --format=format:'%C(bold blue)%h%C(reset) - %C(bold green)(%ar)%C(reset) %C(white)%s%C(reset) %C(dim white)- %an%C(reset)%C(bold yellow)%d%C(reset)' --all
	fullgraph = log --graph --abbrev-commit --decorate --format=format:'%C(bold blue)%h%C(reset) - %C(bold cyan)%aD%C(reset) %C(bold green)(%ar)%C(reset)%C(bold yellow)%d%C(reset)%n''          %C(white)%s%C(reset) %C(dim white)- %an%C(reset)' --all
	review = push origin HEAD:refs/for/master
	st = status
	co = checkout
	br = branch
	up = rebase
	ci = commit
[gitreview]
	remote = origin
